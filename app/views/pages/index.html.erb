<div class="page-container">
  <%= link_to "Send Announcement", new_alert_path %>
  <br><br>
  <%= link_to "Surveys", surveys_path %>
  <br><br>
  <%= link_to "Log Out", session_path(session[:exp_user]), method: :delete, data: { confirm: "Are you sure you want to log out?"} %>
  <br><br>
  <% if flash.any? %>
    <% flash.each do |type, msg| %>
      <p class="error-msg"><%= msg %></p>
    <% end %>
  <% end %>

  <% if @sessions %>
    <% @sessions.each do |session| %>
      <h3><%= session["name"]%></h3>
      <%= session["description"].html_safe %>
      <%= link_to "Edit", edit_page_path(session["id"]) %>
    <% end %>
  <% end %>

  <p>old</p>
  <%= link_to "Access CMS Page", access_path %>

  <%= link_to "Create a New Session", new_page_path %>
</div>
